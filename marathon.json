{
  "id": "/cat-service",
  "instances": 1,
  "cpus": 0.5,
  "mem": 512,
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "anners-docker-registry.bintray.io/cat-service:$tag",
      "network": "BRIDGE",
      "portMappings": [
        {
          "servicePort": 28080,
          "containerPort": 8080,
          "hostPort": 0,
          "protocol": "tcp"
        }
      ],
        "privileged": false,
        "parameters": [],
        "forcePullImage": false
    }
  },
  "upgradeStrategy": {
    "minimumHealthCapacity": 1
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 10,
      "intervalSeconds": 30,
      "maxConsecutiveFailures": 3,
      "path": "/",
      "portIndex": 0,
      "protocol": "HTTP",
      "timeoutSeconds": 5
    }
  ],
  "uris":  [
      "https://s3-us-west-2.amazonaws.com/90d2a58b7f59/.dockercfg"
  ]
}
